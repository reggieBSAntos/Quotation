\r\n  --gradient-primary: linear-gradient(to right bottom, #ff7300, #ff7300);\r\n  --gradient-secondary: linear-gradient(to top left, #ffffff, #41668a);\r\n}\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n  background: var(--gray);\r\n}\r\nbody * {\r\n  margin: 0;\r\n  padding: 0;\r\n\r\n  font-family: sans-serif;\r\n  font-size: 16px;\r\n  color: rgba(0, 0, 0, 0.49);\r\n  box-sizing: border-box;\r\n\r\n  user-select: none;\r\n}\r\nbutton {\r\n  appearance: none;\r\n  -webkit-appearance: none;\r\n  -moz-appearance: none;\r\n  border: none;\r\n  outline: none;\r\n}\r\n.container {\r\n  width: 100%;\r\n  height: 100vh;\r\n\r\n  padding: var(--padding-regular);\r\n}\r\n.panels {\r\n  width: 100%;\r\n  height: 100%;\r\n\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  flex-direction: column;\r\n  gap: var(--padding-regular);\r\n  background: none;\r\n}\r\n.top-panel {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 10%;\r\n  background: var(--primary);\r\n  box-shadow: var(--box-shadow);\r\n  border-radius: 2px;\r\n  padding: calc(var(--padding-regular) * 2) 25px;\r\n\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n.top-panel > h1 {\r\n  flex-grow: 1;\r\n  font-size: 1.25em;\r\n  font-weight: 600;\r\n  color: rgba(0, 0, 0, 0.55);\r\n  /* text-shadow: 2px 2px 3px rgba(0, 0, 0, 0.2); */\r\n}\r\n/* NOTIFCATION */\r\n\r\n.notification-button {\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n\r\n  color: #ffffff;\r\n  outline: none;\r\n  background: none;\r\n\r\n  border-radius: 50%;\r\n  z-index: 102;\r\n}\r\n.notification-button > span:first-of-type {\r\n  color: rgba(0, 0, 0, 0.55);\r\n\r\n  font-size: 2.15em;\r\n}\r\n.notification-button .material-icons {\r\n  color: #ffffff;\r\n}\r\n\r\n.badge {\r\n  position: absolute;\r\n  top: -9px;\r\n  right: -9px;\r\n  width: 22px;\r\n  height: 22px;\r\n  min-width: 22px;\r\n  min-height: 22px;\r\n  max-width: 22px;\r\n  max-height: 22px;\r\n  background: var(--secondary-light);\r\n  box-shadow: var(--box-shadow);\r\n  color: rgba(255, 255, 255, 0.88);\r\n  border-radius: 50%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  cursor: pointer;\r\n  padding: 0;\r\n  font-size: 0.8em;\r\n  /* text-align: center; */\r\n  transition: background 0.25s, transform 0.25s;\r\n}\r\n.badge--hidden {\r\n  display: none;\r\n}\r\n.badge:hover {\r\n  background: var(--secondary-dark);\r\n}\r\n.badge:active {\r\n  transform: scale(0.95, 0.95);\r\n}\r\n.bottom-panel {\r\n  width: 100%;\r\n  height: calc(90% - 10px);\r\n\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n.left-panel {\r\n  height: 100%;\r\n\r\n  background: #fff !important;\r\n\r\n  width: calc(calc((100% * 0.25)) - var(--padding-thin));\r\n  padding: var(--padding-thick);\r\n\r\n  background: none;\r\n  box-shadow: var(--box-shadow);\r\n  border-radius: 2px;\r\n\r\n  overflow: hidden;\r\n}\r\n.left-panel > h4 {\r\n  font-size: 1rem;\r\n  font-weight: 600;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.right-panel {\r\n  width: calc(calc((100% * 0.75)) - var(--padding-thin));\r\n  height: 100%;\r\n  padding: var(--padding-thick);\r\n\r\n  background: #fff !important;\r\n\r\n  box-shadow: var(--box-shadow);\r\n  border-radius: 2px;\r\n}\r\n.right-panel .title {\r\n  padding-top: 16px;\r\n  padding-bottom: 16px;\r\n\r\n  font-size: 24px;\r\n  font-weight: 600;\r\n}\r\n/* SLIDER */\r\n.slider {\r\n  width: 100%;\r\n  height: 100%;\r\n  position: relative;\r\n  background: none;\r\n\r\n  overflow: hidden;\r\n}\r\n\r\n.dots {\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 50%;\r\n\r\n  transform: translateX(-50%);\r\n\r\n  z-index: 10;\r\n}\r\n.dots > div {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  gap: 20px;\r\n}\r\n.dots__dot {\r\n  border: none;\r\n  background: none;\r\n  opacity: 0.7;\r\n  cursor: pointer;\r\n  transition: color 0.2s, opacity 0.2s;\r\n\r\n  color: var(--gray);\r\n\r\n  cursor: pointer;\r\n  /* Only necessary when overlying images */\r\n  /* box-shadow: 0 0.6rem 1.5rem rgba(0, 0, 0, 0.7); */\r\n}\r\n.dots__dot::after {\r\n  font-family: \"Material Icons\";\r\n  content: \"\\e061\";\r\n}\r\n.dots__dot:hover {\r\n  color: var(--gray-dark);\r\n}\r\n.dots__dot--active {\r\n  color: var(--gray-dark);\r\n  opacity: 1;\r\n}\r\n.slider-button {\r\n  position: absolute;\r\n  top: 50%;\r\n  z-index: 10;\r\n\r\n  border: none !important;\r\n  background: none !important;\r\n  color: var(--gray);\r\n\r\n  opacity: 0.7;\r\n  cursor: pointer;\r\n  transition: color 0.2s, opacity 0.2s;\r\n}\r\n.slider-button--prev {\r\n  left: 3%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n.slider-button--prev::after {\r\n  font-family: \"Material Icons\";\r\n  content: \"\\e045\";\r\n  font-size: 2em;\r\n}\r\n.slider-button--next {\r\n  right: 3%;\r\n  transform: translate(50%, -50%);\r\n}\r\n.slider-button--next::after {\r\n  font-family: \"Material Icons\";\r\n  content: \"\\e044\";\r\n  font-size: 2em;\r\n}\r\n.slider-button:hover {\r\n  color: var(--gray-dark);\r\n}\r\n.slider-button:active {\r\n  color: var(--gray-dark);\r\n  opacity: 1;\r\n}\r\n/* CHECKBOX */\r\n.checkbox {\r\n  position: relative;\r\n  display: inline-flex;\r\n  align-items: flex-start;\r\n  justify-content: center;\r\n  cursor: pointer;\r\n}\r\n.checkbox > span {\r\n  color: rgba(0, 0, 0, 0.49) !important;\r\n  font-size: 0.95em;\r\n}\r\n.checkbox__input {\r\n  display: none;\r\n}\r\n.checkbox__box--unchecked {\r\n  transition: background 0.15s, border-color 0.15s;\r\n}\r\n.checkbox__box--checked {\r\n  transition: background 0.15s, border-color 0.15s;\r\n}\r\n.checkbox__box--unchecked::before {\r\n  font-family: \"Material Icons\";\r\n  content: \"\\e15b\";\r\n  margin-right: 5px;\r\n  padding: 2px;\r\n  background: var(--secondary);\r\n  border-radius: 2px;\r\n\r\n  display: block;\r\n  font-size: 0.9em;\r\n  color: #ffffff;\r\n\r\n  transition: color 0.25s;\r\n}\r\n.checkbox__box--checked::after {\r\n  font-family: \"Material Icons\";\r\n  content: \"\\e5ca\";\r\n  margin-right: 5px;\r\n  padding: 2px;\r\n  background: var(--secondary);\r\n  border-radius: 2px;\r\n\r\n  display: block;\r\n  font-size: 0.9em;\r\n  color: #ffffff;\r\n\r\n  transition: color 0.25s;\r\n\r\n  display: none;\r\n}\r\n.checkbox__input:checked + .checkbox__box--checked::after {\r\n  display: block;\r\n}\r\n.checkbox__input:disabled + .checkbox__box--checked::before {\r\n  background: var(--gray);\r\n}\r\n.checkbox__input:checked + .checkbox__box--unchecked::before {\r\n  display: none;\r\n}\r\n.checkbox__input:not(:disabled):hover + .checkbox__box--checked::after {\r\n  background: var(--secondary-dark);\r\n}\r\n.checkbox__input:not(:disabled):hover + .checkbox__box--unchecked::before {\r\n  background: var(--secondary-dark);\r\n}\r\n\r\n/* SLIDE */\r\n.slide {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  top: 0;\r\n  bottom: 5%;\r\n  /* background: red; */\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n\r\n  transition: transform 0.25s ease-out;\r\n  /* THIS creates the animation! */\r\n  /*  animation-duration: 0.35s;\r\n  animation-fill-mode: forwards; */\r\n  overflow-y: auto;\r\n}\r\n.slide--from--left {\r\n  animation-name: slide---from--left;\r\n}\r\n.slide--from--right {\r\n  animation-name: slide---from--right;\r\n}\r\n.slide-left-panel {\r\n  position: relative;\r\n  height: 100%;\r\n  width: 52%;\r\n  padding: 0 25px 10px 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: left;\r\n  font-size: 1.5em;\r\n  justify-content: center;\r\n}\r\n.slide-left-panel > *:not(:last-child) {\r\n  margin-bottom: 10px;\r\n}\r\n.slide-left-panel .checkbox {\r\n  background: var(--secondary);\r\n\r\n  padding: 10px 14px;\r\n  border-radius: 4px;\r\n  box-shadow: var(--box-shadow);\r\n  margin-top: 20px;\r\n}\r\n.slide-left-panel .checkbox:hover {\r\n  background: var(--secondary-dark);\r\n}\r\n.slide-left-panel .checkbox span {\r\n  font-size: 0.9em;\r\n  font-weight: 500;\r\n  text-align: left;\r\n  color: #fff !important;\r\n}\r\n\r\n.text--checked {\r\n  display: none;\r\n}\r\n\r\n.slide-left-panel .checkbox__input:checked ~ .text--checked {\r\n  display: flex;\r\n}\r\n.slide-left-panel .checkbox__input:checked ~ .text--unchecked {\r\n  display: none;\r\n}\r\n\r\n.slide-left-panel .checkbox__box--unchecked::before {\r\n  content: \"\\e145\";\r\n  font-size: 1.1em;\r\n  font-weight: 300;\r\n  color: #fff !important;\r\n\r\n  background: none;\r\n  padding: 0;\r\n  border-radius: 0;\r\n\r\n  transition: display 0.15s;\r\n}\r\n.slide-left-panel .checkbox__box--checked::after {\r\n  content: \"\\e15b\";\r\n  font-weight: 300;\r\n  font-size: 1.1em;\r\n  color: #fff !important;\r\n\r\n  background: none;\r\n  padding: 0;\r\n  border-radius: 0;\r\n\r\n  transition: display 0.15s;\r\n}\r\n\r\n.slide-left-panel > h1 {\r\n  font-size: 1.25rem;\r\n  color: var(--primary);\r\n  text-align: justify;\r\n}\r\n.slide-left-panel > h4 {\r\n  font-size: 1rem;\r\n  font-weight: 500;\r\n  text-align: justify;\r\n}\r\n.slide-right-panel {\r\n  position: relative;\r\n  height: 100%;\r\n  width: 32%;\r\n  padding: 25px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: left;\r\n  justify-content: center;\r\n  gap: 20px;\r\n\r\n  font-size: 0.8em;\r\n}\r\n.slide-right-panel::before {\r\n  content: \"\";\r\n  position: absolute;\r\n  border-left: 1px solid var(--gray);\r\n  height: 80%;\r\n  left: 0;\r\n  top: 0;\r\n  bottom: 0;\r\n\r\n  margin: auto;\r\n}\r\n.slide-fieldset {\r\n  border: none;\r\n}\r\n.slide-fieldset__legend {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-bottom: 20px;\r\n}\r\n.slide-fieldset__legend > span,\r\n.slide-fieldset__legend > span > strong {\r\n  color: rgba(0, 0, 0, 0.49);\r\n  font-size: 0.95em;\r\n}\r\n.slide-fieldset > div:not(:last-child) {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n@keyframes slide---from--left {\r\n  from {\r\n    transform: translateX(0);\r\n  }\r\n  to {\r\n    transform: translateX(-100%);\r\n  }\r\n}\r\n@keyframes slide---from--right {\r\n  from {\r\n    transform: translateX(100%);\r\n  }\r\n  to {\r\n    transform: translateX(0);\r\n  }\r\n}\r\n@keyframes slide---to--left {\r\n  from {\r\n    transform: translateX(-100%);\r\n  }\r\n  to {\r\n    transform: translateX(0);\r\n  }\r\n}\r\n@keyframes slide---to--right {\r\n  from {\r\n    transform: translateX(0);\r\n  }\r\n  to {\r\n    transform: translateX(100%);\r\n  }\r\n}\r\n\r\n.accordion {\r\n  width: 100%;\r\n  max-height: 100%;\r\n  overflow: auto;\r\n  font-family: sans-serif;\r\n}\r\n\r\n.accordion__label,\r\n.accordion__content {\r\n  padding: 3px;\r\n}\r\n.accordion__label {\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n  cursor: pointer;\r\n  transition: color 0.1s;\r\n\r\n  padding: 10px 6px;\r\n  border-radius: 4px;\r\n  box-shadow: var(--box-shadow);\r\n\r\n  margin-bottom: 4px;\r\n  color: rgba(0, 0, 0, 0.66);\r\n\r\n  background: var(--primary);\r\n\r\n  overflow: hidden;\r\n\r\n  transition: background 0.25s;\r\n}\r\n/* .accordion__label:hover {\r\n  border-color: var(--primary-light);\r\n} */\r\n.accordion__label::after {\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  right: 6px;\r\n  display: flex;\r\n  align-items: center;\r\n  margin: auto;\r\n  font-family: \"Material Icons\";\r\n  font-size: 1.2em;\r\n  content: \"\\e5cf\";\r\n\r\n  transition: transform 0.2s;\r\n}\r\n\r\n.accordion__content {\r\n  display: none;\r\n}\r\n.accordion__input {\r\n  display: none;\r\n}\r\n.accordion__input:checked ~ .accordion__content {\r\n  display: block;\r\n}\r\n\r\n.accordion__input:checked ~ .accordion__label::after {\r\n  transform: rotate(0.5turn);\r\n}\r\n\r\n.service {\r\n  padding: 10px 6px;\r\n\r\n  cursor: pointer;\r\n  transition: backgrond 0.2s;\r\n}\r\n.service__title {\r\n  color: rgba(0, 0, 0, 0.66);\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.service__title strong {\r\n  color: rgba(0, 0, 0, 0.55);\r\n}\r\n.service__title::before {\r\n  content: \"\\e061\";\r\n  font-family: \"Material Icons\";\r\n  font-size: 0.7em;\r\n  margin-right: 5px;\r\n}\r\n.service:hover {\r\n  background: rgba(0, 0, 0, 0.03);\r\n  border-radius: 4px;\r\n}\r\n\r\n@keyframes form---close {\r\n  to {\r\n    transform: scale(0);\r\n  }\r\n}\r\n.arrow-back {\r\n  cursor: pointer;\r\n  color: var(--gray);\r\n  transition: color 0.2s, opacity 0.2s;\r\n  opacity: 0.7;\r\n}\r\n.arrow-back::before {\r\n  font-family: \"Material Icons\";\r\n  content: \"\\e5c4\";\r\n  font-size: 1.5em;\r\n}\r\n.arrow-back:hover {\r\n  color: var(--gray-dark);\r\n}\r\n.arrow-back:active {\r\n  color: var(--gray-dark);\r\n  opacity: 1;\r\n}\r\n.instructions {\r\n  margin-top: 16px;\r\n  max-width: 600px;\r\n}\r\n.contents {\r\n  margin-top: 16px;\r\n  border: none;\r\n  outline: none;\r\n}\r\n.contents > legend {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-bottom: 8px;\r\n\r\n  font-weight: 600;\r\n}\r\n.contents > ol {\r\n  max-width: 600px;\r\n  padding: 0 20px;\r\n}\r\n.contents .content:not(:last-of-type) {\r\n  margin-bottom: 16px;\r\n}\r\n.content {\r\n  margin-bottom: 8px;\r\n}\r\n.content:last-of-type {\r\n  margin-bottom: 16px;\r\n}\r\n.notes {\r\n  font-size: 0.9em;\r\n  font-style: italic;\r\n}\r\n.question,\r\n.notes > p {\r\n  line-height: 1.15;\r\n}\r\n.response {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 6px;\r\n}\r\n\r\n/*****RADIO*****/\r\n.radio {\r\n  position: relative;\r\n  display: inline-flex;\r\n  align-items: center;\r\n  justify-content: left;\r\n  cursor: pointer;\r\n  padding-left: 1.5em;\r\n}\r\n.radio__input {\r\n  display: none;\r\n}\r\n.radio__radio {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  margin: auto;\r\n  width: 1.15em;\r\n  height: 1.15em;\r\n  border: 1px solid #d9d9d9;\r\n  border-radius: 50%;\r\n  margin-right: 10px;\r\n  box-sizing: border-box;\r\n  padding: 2px;\r\n  transition: background 0.15s;\r\n}\r\n.radio__radio::after {\r\n  content: \"\";\r\n  width: 100%;\r\n  height: 100%;\r\n  display: block;\r\n  background: #ff7300;\r\n  border-radius: 50%;\r\n  transform: scale(0);\r\n  transition: transform 0.15s;\r\n}\r\n.radio__input:checked + .radio__radio::after {\r\n  transform: scale(1);\r\n}\r\n\r\n/* AVERAGE SCREEN HEIGHT OF MOBILE DEVICES - 800*/\r\n/* FROM 720 */\r\n@media (max-width: 428px), (min-height: 813px) {\r\n  body {\r\n    height: 100vh;\r\n    background: none;\r\n  }\r\n  .container {\r\n    height: 100%;\r\n    height: calc(var(--vh, 1vh) * 100);\r\n    padding: 0;\r\n    background: none;\r\n    background: var(--primary);\r\n    gap: 0;\r\n  }\r\n  .panels {\r\n    gap: 0;\r\n    justify-content: flex-start;\r\n  }\r\n  .top-panel {\r\n    width: 100%;\r\n    padding: var(--padding-thick);\r\n    background: none;\r\n    box-shadow: none;\r\n    border-radius: 0;\r\n  }\r\n  .top-panel > h1 {\r\n    font-size: 1.35em;\r\n    font-weight: 600;\r\n  }\r\n  .bottom-panel {\r\n    flex-grow: 1;\r\n    background: none;\r\n    gap: 0;\r\n  }\r\n  .left-panel {\r\n    display: none;\r\n  }\r\n  .right-panel {\r\n    width: 100%;\r\n    display: block;\r\n    padding: 0;\r\n    background: rgba(255, 255, 255, 0.95) !important;\r\n    box-shadow: none;\r\n    border-radius: 0;\r\n  }\r\n  .slider {\r\n    height: 100%;\r\n    width: 100%;\r\n    position: relative;\r\n    overflow: hidden;\r\n  }\r\n  .slider-button {\r\n    display: none;\r\n  }\r\n  .slide {\r\n    display: block;\r\n  }\r\n  .slide-left-panel,\r\n  .slide-right-panel {\r\n    height: auto;\r\n    width: 100% !important;\r\n    display: block;\r\n    padding: var(--padding-thick);\r\n  }\r\n  .slide-left-panel {\r\n    text-align: center;\r\n  }\r\n  /* .slide-left-panel .checkbox span {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  } */\r\n\r\n  .slide-right-panel {\r\n    padding-top: var(--padding-thick);\r\n  }\r\n  .slide-right-panel::before {\r\n    display: none;\r\n  }\r\n  .dots {\r\n    bottom: 1%;\r\n  }\r\n}\r\n@media (max-width: 428px) {\r\n  .form__button,\r\n  .dateInp,\r\n  .nameInp,\r\n  .emailInp {\r\n    width: 100%;\r\n  }\r\n}\r\n"],"names":[],"version":3,"file":"index.60928911.css.map","sourceRoot":"/__parcel_source_root/"}